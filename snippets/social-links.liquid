<ul class="social-links">
	{% if settings.social_facebook_link != blank %}
	<li><a href="{{ settings.social_facebook_link }}" class="social facebook" target="_blank" rel="noreferrer" title="Facebook">{% render 'svg-icons' with 'facebook' %} 
	{% comment %} <span>{{ 'social.links.facebook' | t }}</span> {% endcomment %}
	</a></li>
	{% endif %}
