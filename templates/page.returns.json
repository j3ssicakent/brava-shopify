    "4896a34d-7fef-44ef-a30a-73625d646d87": {
      "type": "rich-text",
      "blocks": {
        "fdc10973-fb14-41a4-a4dd-2567d08307df": {
          "type": "text",
          "settings": {
            "text": "<h3><strong>Return Policy<\/strong><\/h3>",
            "size": "text-size-large"
          }
        },
        "30ab381a-9511-4eec-8d9b-d0798cd9609d": {
          "type": "text",
          "settings": {
            "text": "<p><em>Only unworn and unwashed clothing and accessories returned in their original packaging are eligible for our return policy. Home products must be in brand new condition and shipped back in their original packaging.<\/em><\/p>",
            "size": "text-size-large"
          }
        },
        "c0a45e18-a39b-4952-b9a8-9b370df410dc": {
          "type": "text",
          "settings": {
            "text": "<p><strong>If sending within North and South America:<\/strong> 14 days from the date your item(s) has been delivered.<br\/><strong>If sending within Europe:<\/strong> 21 days from the date your item(s) has been delivered.<br\/><strong>If sending from Rest of World:<\/strong> 31 days from the date your item(s) has been delivered.<\/p>",
            "size": "text-size-large"
          }
        },
        "ff09c7a7-c761-42f3-aa3a-3bd268d996d8": {
          "type": "text",
          "settings": {
            "text": "<p>Need support? Contact us down below or directly at <a href=\"mailto:info@bravaland.com\"><span style=\"text-decoration:underline\">info@bravaland.com<\/span><\/a> for assistance. We’ll get your note either way!<\/p>",
            "size": "text-size-large"
          }
        }
      },
