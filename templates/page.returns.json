      "blocks": {
        "template--18112915374375__dabb125b-c46f-4e06-87d6-5d5792cf3369-1684170311b0911365-2": {
          "type": "text",
          "settings": {
            "text": "<h6>Oh no! Something not quite right with your new gear!?<\/h6>",
            "size": "text-size-large"
          }
        },
        "53dea7c0-81e7-409c-a4af-d2ad2986034f": {
          "type": "text",
          "settings": {
            "text": "<p>No problem! These things happen and we bet we’ll do better next time.<\/p>",
            "size": "text-size-large"
          }
        },
        "7fdccddd-983b-4b29-bcff-10e1f761cd73": {
          "type": "text",
          "settings": {
            "text": "<p>Let’s get you a refund back. Better yet - let’s get you something different!<\/p>",
            "size": "text-size-large"
          }
        },
        "aec8d744-307a-4dd9-9ccc-7d0da979fd5f": {
          "type": "text",
          "settings": {
            "text": "<p><strong>Simply request a prepaid return label and send it back. You can either wait for your refund to process or submit a new order while you wait. <\/strong>Easy squeezy!<\/p><p><br\/>At this time, we do not offer exchanges.<\/p>",
            "size": "text-size-large"
          }
        }
      },
      "block_order": [
        "template--18112915374375__dabb125b-c46f-4e06-87d6-5d5792cf3369-1684170311b0911365-2",
