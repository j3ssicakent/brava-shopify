      }
    },
    "440ff28c-311b-4543-b9cc-74a83baf69e3": {
      "type": "rich-text",
      "blocks": {
        "b0a52d64-355c-4ea9-b950-73fd774115c5": {
          "type": "heading",
          "settings": {
            "text": "Usage",
            "size": "h3"
          }
        },
        "d48609fd-5668-4850-88ad-16efa7ee3dbd": {
          "type": "text",
          "settings": {
            "text": "<p>The creator grants full ownership, copyright, and usage rights of all submitted content to brava upon creator’s acceptance of entry into the program. The creator is encouraged to share content across his \/ her own social media channels, tagging brava, and encouraging others to support brava immediately.<\/p>",
            "size": "text-size-regular"
          }
        }
      },
      "block_order": [
        "b0a52d64-355c-4ea9-b950-73fd774115c5",
        "d48609fd-5668-4850-88ad-16efa7ee3dbd"
      ],
