    "1662373200967c613c": {
      "type": "rich-text",
      "blocks": {
        "18fca2b3-2d26-458f-9966-5e1094982b3e": {
          "type": "heading",
          "settings": {
            "text": "Eligibility",
            "size": "h3"
          }
        },
        "6ff53987-13c5-4fce-ae9a-57516f8227da": {
          "type": "text",
          "settings": {
            "text": "<p>Content creators will apply to be part of our program. They will submit (3) pieces of content suited for X, IG, and \/ or TikTok, including at least (1) video. English must be used across all relevant content.<\/p>",
            "size": "text-size-regular"
          }
        }
      },
      "block_order": [
        "18fca2b3-2d26-458f-9966-5e1094982b3e",
