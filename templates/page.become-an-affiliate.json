    "4d1f6169-4b16-4e0f-a6a8-4c4899c39dcc": {
      "type": "rich-text",
      "blocks": {
        "affe598a-b735-4568-8934-507a7bbddaca": {
          "type": "heading",
          "settings": {
            "text": "Get Paid",
            "size": "h3"
          }
        },
        "4076afff-1fd9-442c-a9ab-e70967fb38a9": {
          "type": "text",
          "settings": {
            "text": "<p>Payments are issued from us to you via PayPal each month. If you have any issues with PayPal, please contact their support team.<\/p>",
            "size": "text-size-large"
          }
        },
        "9350d6e4-31b8-4555-97bb-70a2cf3c006e": {
          "type": "text",
          "settings": {
            "text": "<p>For questions about our affiliate program, please contact us at <a href=\"mailto:info@bravaland.com\"><span style=\"text-decoration:underline\">info@bravaland.com<\/span><\/a> or DM us on Twitter <a href=\"http:\/twitter.com\/mybravaland\" target=\"_blank\" title=\"Twitter\">@mybravaland<\/a>!<\/p>",
            "size": "text-size-large"
          }
        },
        "93618fb9-35d6-4d3a-90fd-ef6bcda18dbd": {
          "type": "text",
          "settings": {
            "text": "<h6><a href=\"https:\/\/portal.bravaland.com\/#\/login\" target=\"_blank\" title=\"brava affiliate portal\">Now go get that affiliate link!<\/a><\/h6>",
            "size": "text-size-large"
          }
        }
      },
